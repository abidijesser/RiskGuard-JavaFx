    <center>
        <ListView fx:id="assuranceListView" prefHeight="300.0" prefWidth="300.0" />
    </center>
    <bottom>
        <HBox spacing="10.0" alignment="CENTER">
            <Button text="Supprimer Assurance Véhicule" onAction="#supprimerAssuranceVehicule" />
            <Button text="Update" onAction="#updateAssuranceVehicule" />
            <Button text="Reload" onAction="#reloadAssuranceVehicules" />
        </HBox>
    </bottom>
</BorderPane>
