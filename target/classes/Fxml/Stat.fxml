<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<?import java.net.URL?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.assurance.assurancess.controller.stat">
    <stylesheets>
        <URL value="@css/Stat.css"></URL>
    </stylesheets>
    <PieChart fx:id="pieChart" layoutX="50.0" layoutY="50.0" prefHeight="300.0" prefWidth="300.0" />
    <Label fx:id="top3Label" layoutX="400.0" layoutY="50.0" style="-fx-font-size: 14pt; -fx-font-weight: bold; -fx-text-fill: #FFFFFF; -fx-background-color: #007bff; -fx-padding: 5px; -fx-border-radius: 5px;" />
    <Button layoutX="450.0" layoutY="350.0" onAction="#gotoOtherStatPage" text="Other Stat" />
    <Button layoutX="7.0" layoutY="8.0" onAction="#goback" text="&lt;" />

</AnchorPane>
